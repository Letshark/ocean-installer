{
    "name": "letshark/ocean-installer",
    "description": "Ocean Installer CLI tool for creating Ocean projects",
    "type": "composer-plugin",
    "keywords": ["ocean", "installer", "cli", "tool"],
    "license": "MIT",
    "bin": ["bin/ocean"],
    "autoload": {
        "psr-4": {
        "Ocean\\Installer\\": "src/"
        }
    },
    "require": {
        "php": ">=8.2",
        "symfony/console": "^7.0",
        "guzzlehttp/guzzle": "^7.8",
        "composer-plugin-api": "^2.0"
    },
    "extra": {
        "class": "Ocean\\Installer\\InstallPlugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}